<h1>Associated Attributes - Event</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>General Info</th>
      <th>Event Info</th>
    </tr>
  </thead>
  <tbody>
    <% @associated_attrs.each do |attr| %>
      <% if attr.associated_endpoints.include?("Event") %>
        <tr>
          <td><%= attr.attr_title %></td>
          <td><%= attr.general_info %></td>
          <td><%= attr.event_info %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>