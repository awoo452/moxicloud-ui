<h1>Associated Attributes - EmailCampaign</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>General Info</th>
      <th>EmailCampaign Info</th>
    </tr>
  </thead>
  <tbody>
    <% @associated_attrs.each do |attr| %>
      <!-- Check if the associated_endpoints field includes "ActionLog" -->
      <% if attr.associated_endpoints.include?("EmailCampaign") %>
        <tr>
          <td><%= attr.attr_title %></td>
          <td><%= attr.general_info %></td>
          <td><%= attr.emailcampaign_info %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>